#
# this config shows that we cannot parse a backup missing
# an opening curly brace
#

yabsm_dir=/#always required

subvol home {

    mountpoint=/home

    5minute_want=no
    5minute_keep=0

    hourly_want=no
    hourly_keep=0

    midnight_want=no
    midnight_keep=0

    monthly_want=no
    monthly_keep=0
}

backup homeBackup # missing opening curly brace

    remote=no
    
    subvol=home

    backup_dir=/

    timeframe=hourly

    keep=100
}
