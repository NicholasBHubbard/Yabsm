#
# This config show we cannot leave out a TFRAME_time setting
#

backup homeBackup {
    timeframe=daily
    ### time=23:59 # REQUIRED

    subvol=home
    remote=no
    backup_dir=/home
    keep=12
}

subvol home {
    mountpoint=/home
    5minute_want=no
    hourly_want=no
    daily_want=no
    weekly_want=no
    monthly_want=no
}

