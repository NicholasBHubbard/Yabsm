#
# this config shows that we cannot parse a backup missing
# an opening curly brace
#

yabsm_dir=/#always required

subvol home {
    mountpoint=/home
    5minute_want=no
    hourly_want=no
    midnight_want=no
    weekly_want=no
    monthly_want=no
}

backup homeBackup # missing opening curly brace
    remote=no
    subvol=home
    backup_dir=/
    timeframe=hourly
    keep=100
}
