#
# This config shows that a subjects closing brace can touch the last setting
#

yabsm_dir=/ # always required

subvol home {
       mountpoint=/home
       5minute_want=no
       hourly_want=no
       midnight_want=no
       weekly_want=no
       monthly_want=no} # touching

backup homeBackup {
       subvol=home
       remote=no
       backup_dir=/
       keep=100
       timeframe=midnight} # touching