#
# this config shows that we can define backup settings in any order
#

yabsm_dir=/#always required

subvol home {
    mountpoint=/home
    5minute_want=no
    5minute_keep=0
    hourly_want=no
    hourly_keep=0
    midnight_want=no
    midnight_keep=0
    monthly_want=no
    monthly_keep=0
}

backup homeBackup {
    host=foobar
    keep=100
    subvol=home
    timeframe=hourly
    backup_dir=/
    remote=yes
}
