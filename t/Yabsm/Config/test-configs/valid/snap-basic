# This config defines a snap with max timeframes.

subvol foo {
    mountpoint=/
}

snap foo {
    subvol=foo
    dir=/.snapshots/yabsm/foo
    timeframes=5minute,hourly,daily,weekly,monthly
    daily_time=23:59
    weekly_day=wednesday
    weekly_time=00:00
    monthly_day=31
    monthly_time=23:59
    5minute_keep=36
    hourly_keep=48
    daily_keep=365
    weekly_keep=56
    monthly_keep=12
}