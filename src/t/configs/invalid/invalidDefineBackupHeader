#
# this config shows that we do not parse invalid define_backup headers
#

yabsm_snapshot_dir=/#always required

define_subvol home {

    mountpoint=/home

    5minute_want=no
    5minute_keep=0

    hourly_want=no
    hourly_keep=0

    midnight_want=no
    midnight_keep=0

    monthly_want=no
    monthly_keep=0
}

define_backup homeBackup # missing opening curly brace

    remote=no
    
    subvol=home

    backup_dir=/

    timeframe=hourly

    keep=100
}
