#
# This config shows we cannot leave out a weekly_time setting for a subvol
#

yabsm_dir=/#always required

subvol home {
    weekly_want=yes
    weekly_day=monday
    weekly_keep=12
    ### weekly_time=23:59 # REQUIRED

    mountpoint=/home
    5minute_want=no
    hourly_want=no
    daily_want=no
    monthly_want=no
}