#
# This config shows that we cannot omit a closing curly brace
# in a define subvol block
#

yabsm_snapshot_dir=/

define_subvol home {

    mountpoint=/home

    5minute_want=no
    5minute_keep=0

    hourly_want=no
    hourly_keep=0

    midnight_want=no
    midnight_keep=0

    monthly_want=no
    monthly_keep=0
