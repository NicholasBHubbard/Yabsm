yabsm_snapshot_dir=/#always required

define_subvol home {

mountpoint=/home

midnight_want=no
midnight_keep=0

monthly_want=no
monthly_keep=0

hourly_want=no
hourly_keep=0

5minute_want=no
5minute_keep=0

}

define_backup homeBackup {

remote=no	      

subvol=home

backup_dir=/

timeframe=hourly

keep=100
}
