#
# This config shows we cannot omit a closing curly brace in a
# define backup block.
#

yabsm_dir=/#always required

subvol home {
    mountpoint=/home
    5minute_want=no
    hourly_want=no
    midnight_want=no
    weekly_want=no
    monthly_want=no
}

backup homeBackup {
    remote=no
    subvol=home
    backup_dir=/
    timeframe=hourly
    keep=100

#missing closing brace
