# This config should fail because we are defining two
# snaps with the same name.

subvol foo {
    mountpoint=/
}

snap foo {
    subvol=foo
    dir=/.snapshots/yabsm/foo
    timeframes=5minute
    5minute_keep=36
}

snap foo {
    subvol=foo
    dir=/.snapshots/yabsm/bar
    timeframes=5minute
    5minute_keep=1
}