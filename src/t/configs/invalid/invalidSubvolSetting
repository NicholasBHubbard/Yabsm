#
# this config shows that we cannot define a subvol with an invalid setting
#

yabsm_snapshot_dir=/#always required

define_subvol home {

    some_invalid_setting=whatever

    mountpoint=/home

    5minute_want=no
    5minute_keep=0

    hourly_want=no
    hourly_keep=0

    midnight_want=no
    midnight_keep=0

    monthly_want=no
    monthly_keep=0
}
