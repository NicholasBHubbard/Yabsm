yabsm_dir=/#always required

subvol home {
mountpoint=/home
midnight_want=no
midnight_keep=0
monthly_want=no
monthly_keep=0
hourly_want=no
hourly_keep=0
5minute_want=no
5minute_keep=0
}

backup homeBackup {
remote=no	      
subvol=home
backup_dir=/
