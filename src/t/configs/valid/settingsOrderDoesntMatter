#
# This config shows that the the order we define settings doesn't matter
#

subvol home {
       weekly_day=monday
       5minute_keep=100
       mountpoint=/home
       hourly_want=yes
       weekly_time=14:40
       monthly_keep=100
       5minute_want=yes
       daily_keep=100
       daily_time=23:59
       monthly_want=yes
       weekly_keep=100
       hourly_keep=100
       daily_want=yes
       monthly_time=13:30
       weekly_want=yes
}

yabsm_dir=/

backup homeBackup {
       timeframe=5minute
       day=monday
       host=foobar
       backup_dir=/
       keep=100
       subvol=home
       remote=yes
}
